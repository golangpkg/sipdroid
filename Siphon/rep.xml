<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>info</key>
	  <dict>
	    <key>name</key>
	    <string>Touchmods</string>
	    <key>maintainer</key>
	    <string>samuel,marian,metabaron</string>
	    <key>contact</key>
	    <string>siphon@laposte.net</string>
	    <key>url</key>
	    <string>http://touchmods.org/</string>
	    <key>category</key>
	    <string>Touchmods</string>
	    <key>description</key>
	    <string>Touchmods Repository - Home of the 1st free SIP-VoIP Application for iPhone and iPod Touch</string>
		</dict>
		<key>packages</key>
		<array>
	    <dict>
	      <key>bundleIdentifier</key>
	      <string>org.touchmods.Siphon</string>
	      <key>description</key>
	      <string>Touchmods free SIP-VoIP Application for iPhone and iPod Touch</string>
	      <key>hash</key>
	      <string>@CHECKSUM@</string>
	      <key>location</key>
	      <string>http://svsip.free.fr/repository/Siphon.@VERSION@.zip</string>
	      <key>name</key>
	      <string>Siphon</string>
	      <key>scripts</key>
	      <dict>
	        <key>install</key>
	          <array>
	            <array>
	              <string>CopyPath</string>
	              <string>Siphon.app</string>
	              <string>/Applications/Siphon.app</string>
	            </array>
	            <array>
	              <string>CopyPath</string>
	              <string>SiphonSettings.bundle</string>
	              <string>/System/Library/PreferenceBundles/SiphonSettings.bundle</string>
	            </array>
	            <array>
	              <string>CopyPath</string>
	              <string>SpringBoard.app</string>
	              <string>/System/Library/CoreServices/SpringBoard.app</string>
	            </array>
	            <array>
                  <string>Exec</string>
                  <string>/Applications/Siphon.app/Siphon --installPrefBundle</string>
                </array>
	          </array>
	        <key>uninstall</key>
	          <array>
	            <array>
                  <string>Exec</string>
                  <string>/Applications/Siphon.app/Siphon --removePrefBundle</string>
                </array>
	            <array>
	              <string>RemovePath</string>
	              <string>/Applications/Siphon.app</string>
	            </array>
	            <array>
	              <string>RemovePath</string>
	              <string>/System/Library/PreferenceBundles/SiphonSettings.bundle</string>
	            </array>
	            <array>
	              <string>RemovePath</string>
	              <string>/System/Library/CoreServices/SpringBoard.app/Default_Siphon.png</string>
	            </array>
	            <array>
	              <string>RemovePath</string>
	              <string>/System/Library/CoreServices/SpringBoard.app/FSO_Siphon.png</string>
	            </array>
	          </array>
	        <key>update</key>
	          <array>
	            <array>
	              <string>RemovePath</string>
	              <string>/Applications/Siphon.app</string>
	            </array>
	            <array>
	              <string>RemovePath</string>
	              <string>/System/Library/PreferenceBundles/SiphonSettings.bundle</string>
	            </array>
	            <array>
	              <string>CopyPath</string>
	              <string>Siphon.app</string>
	              <string>/Applications/Siphon.app</string>
	            </array>
	            <array>
	              <string>CopyPath</string>
	              <string>SiphonSettings.bundle</string>
	              <string>/System/Library/PreferenceBundles/SiphonSettings.bundle</string>
	            </array>
	            <array>
	              <string>CopyPath</string>
	              <string>SpringBoard.app</string>
	              <string>/System/Library/CoreServices/SpringBoard.app</string>
	            </array>
	          </array>
	      </dict>
	      <key>size</key>
	      <string>@SIZE@</string>
	      <key>url</key>
	      <string>http://code.google.com/p/siphon</string>
	      <key>version</key>
	      <string>@VERSION@</string>
	      <key>category</key>
	      <string>Touchmods</string>
	      <key>date</key>
	      <string>@DATE@</string>
	    </dict>
		</array>
	</dict>
</plist>
